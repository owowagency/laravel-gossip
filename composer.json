{
    "name": "owowagency/laravel-gossip",
    "description": "A package of conversation features for the Laravel framework.",
    "type": "library",
    "authors": [
        {
            "name": "Albert Pratomo",
            "email": "albert@owow.io"
        },
        {
            "name": "Thomas van der Veen",
            "email": "thomas@owow.io"
        },
        {
            "name": "Dees Oomens",
            "email": "dees@owow.io"
        }
    ],
    "keywords": [
        "Laravel",
        "Gossip",
        "Converstaion"
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0",
        "owowagency/laravel-media": "^v3.0",
        "owowagency/laravel-resources": "^3.0",
        "spatie/laravel-permission": "^5.7",
        "spatie/laravel-query-builder": "^5.1"
    },
    "require-dev": {
        "orchestra/testbench": "^7.19",
        "phpunit/phpunit": "^9.0",
        "owowagency/laravel-test-response": "^0.0.6",
        "owowagency/phpunit-snapshot-assertions": "^0.0.8",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "OwowAgency\\Gossip\\": "src/",
            "OwowAgency\\Gossip\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/Support/Database/Factories/UserFactory.php"
        ],
        "psr-4": {
            "OwowAgency\\Gossip\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OwowAgency\\Gossip\\ServiceProvider"
            ]
        }
    }
}
